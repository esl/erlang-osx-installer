	<string>alert</string>
	<key>OSAScriptingDefinition</key>
	<string>ErlangInstaller.sdef</string>
	<key>NSAppleScriptEnabled</key>
	<true/>
	<key>LSUIElement</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
