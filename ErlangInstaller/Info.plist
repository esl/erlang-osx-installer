	<string>MainMenu</string>
	<key>ReleasesUrl</key>
	<string>$(RELEASES_URL)</string>
	<key>SUFeedURL</key>
	<string>$(UPDATES_URL)</string>
	<key>BaseTarballUrl</key>
	<string>$(BASE_TARBALL_URL)</string>
